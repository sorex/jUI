<script type="text/javascript">
    $(function () {
        $("#scrollDiv").jscrollbar({ showWidth: 300, showHeight: 300 });
        //$("#scrollDiv").jscrollbar();
        //#region 取值
        var buttonDefault = $("#scrollDiv").jscrollbar("option","buttonDefault");
        var buttonActive = $("#scrollDiv").jscrollbar("option", "buttonActive");
        var buttonHover = $("#scrollDiv").jscrollbar("option", "buttonHover");
        var buttonBorder = $("#scrollDiv").jscrollbar("option", "buttonBorder");
        var buttonWidth = $("#scrollDiv").jscrollbar("option", "buttonWidth");
        var buttonHeight = $("#scrollDiv").jscrollbar("option", "buttonHeight");

        var handleDefault = $("#scrollDiv").jscrollbar("option", "handleDefault");
        var handleActive = $("#scrollDiv").jscrollbar("option", "handleActive");
        var handleHover = $("#scrollDiv").jscrollbar("option", "handleHover");

        var grooveColor = $("#scrollDiv").jscrollbar("option", "grooveColor");
        var grooveBorder = $("#scrollDiv").jscrollbar("option", "grooveBorder");
        var grooveWidth = $("#scrollDiv").jscrollbar("option", "grooveWidth");

        var showWidth = $("#scrollDiv").jscrollbar("option", "showWidth");
        var showHeight = $("#scrollDiv").jscrollbar("option", "showHeight");
        var handleWidth = $("#scrollDiv").jscrollbar("option", "handleWidth");
        var grooveRadius = $("#scrollDiv").jscrollbar("option", "grooveRadius");
        var handleRadius = $("#scrollDiv").jscrollbar("option", "handleRadius");
        //#endregion
        //#region赋值 input
        $("#txt_btnDefault").attr("value", buttonDefault);
        $("#txt_btnActive").attr("value", buttonActive);
        $("#txt_btnHover").attr("value", buttonHover);
        $("#txt_buttonBorder").attr("value", buttonBorder);
        $("#txt_buttonWidth").attr("value", buttonWidth);
        $("#txt_buttonHeight").attr("value", buttonHeight);

        $("#txt_handleDefault").attr("value", handleDefault);
        $("#txt_handleActive").attr("value", handleActive);
        $("#txt_handleHover").attr("value", handleHover);

        $("#txt_grooveColor").attr("value", grooveColor);
        $("#txt_grooveBorder").attr("value", grooveBorder);
        $("#txt_grooveWidth").attr("value", grooveWidth);

        $("#txt_showWidth").attr("value", showWidth);
        $("#txt_showHeight").attr("value", showHeight);
        $("#txt_handleWidth").attr("value", handleWidth);
        $("#txt_grooveRadius").attr("value", grooveRadius);
        $("#txt_handleRadius").attr("value", handleRadius);
        //#endregion
        //#region赋值 span
        $("#sp_btnDefault").css("background-color", buttonDefault);
        $("#sp_btnActive").css("background-color", buttonActive);
        $("#sp_btnHover").css("background-color", buttonHover);
        $("#sp_buttonBorder").css("background-color", buttonBorder);

        $("#sp_handleDefault").css("background-color", handleDefault);
        $("#sp_handleActive").css("background-color", handleActive);
        $("#sp_handleHover").css("background-color", handleHover);

        $("#sp_grooveColor").css("background-color", grooveColor);
        $("#sp_grooveBorder").css("background-color", grooveBorder);

        $("#sp_grooveRadius").css("border-radius", grooveRadius + "px");
        $("#sp_handleRadius").css("border-radius", handleRadius + "px");

        //#endregion
        //#region文本框
        $("input").click(function () {
            $(this).select();
        }).focusout(function () {
            var new_value = $(this).attr("value");
            if ($(this).attr("id") == "txt_grooveWidth") {
                new_value = parseInt($("#txt_grooveWidth").attr("value"));
            }
            if ($(this).attr("id") == "txt_handleRadius") {
                new_value = parseInt($("#txt_handleRadius").attr("value"));
                $(this).next().css("border-radius", new_value + "px");
            }
            if ($(this).attr("id") == "txt_showWidth") {
                new_value = parseInt($("#txt_showWidth").attr("value"));
            }
            if ($(this).attr("id") == "txt_showHeight") {
                new_value = parseInt($("#txt_showHeight").attr("value"));
            }
            if ($(this).attr("id") == "txt_grooveRadius") {
                new_value = parseInt($("#txt_grooveRadius").attr("value"));
                $(this).next().css("border-radius", new_value + "px");
            }
            if ($(this).attr("id") == "txt_handleWidth") {
                new_value = parseInt($("#txt_handleWidth").attr("value"));
            }
            if ($(this).attr("id") == "txt_buttonWidth") {
                new_value = parseInt($("#txt_buttonWidth").attr("value"));
            }
            if ($(this).attr("id") == "txt_buttonHeight") {
                new_value = parseInt($("#txt_buttonHeight").attr("value"));
            }
            $("#scrollDiv").jscrollbar("option", $(this).attr("id").substring(4), new_value);
            $(this).next().css("background-color", new_value);
            $("#showControll").width(300);
            $("#showControll").height(300);
        });
        //#endregion
        //设置样式控制模块宽度与高度
        $("#showControll").width(300);
        $("#showControll").height(300);
    })
</script>
